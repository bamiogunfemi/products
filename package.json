{
  "name": "products",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel -d ./build ./src -s",
    "clean": "rm -rf .nyc_output build coverage && mkdir build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint \"**/*.js\" --fix",
    "postinstall": "npm run build",
    "start": "node ./build/index.js",
    "start:dev": "set DEBUG=dev && nodemon --exec babel-node ./src/index.js",
    "test": "cross-env NODE_ENV=test nyc mocha --require @babel/register --timeout 1000 ./test/index.js --exit && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetKazuki/products.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/meetKazuki/products/issues"
  },
  "homepage": "https://github.com/meetKazuki/products#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "cross-env": "^5.2.1",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "nyc": "^14.1.1"
  }
}
